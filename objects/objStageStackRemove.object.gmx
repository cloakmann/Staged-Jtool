            <string>///remove current map from storage
if !scrTriggerMenu() exit;
with objStageStackMenu {
    if ds_map_empty(mapStorage)exit;
    var newKey;
    
    //if current map is the only map in storage
    if ds_map_size(mapStorage) == 1 {
        newKey = "startup.jmap";
        //delete current key and load a startup map as first map
        ds_map_delete(mapStorage,currentMapKey);
        loadMapFromStorage(defaultStartupMap,newKey);
    }
    else {
        //if currently the last map, load first map
        if currentMapKey == ds_map_find_last(mapStorage) {
            newKey = ds_map_find_first(mapStorage);
        }
        //else load next map
        else newKey = ds_map_find_next(mapStorage,currentMapKey);
        ds_map_delete(mapStorage,currentMapKey);
        loadMapFromStorage(mapStorage[?newKey],newKey);
    }
}
</string>
